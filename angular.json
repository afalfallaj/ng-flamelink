{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tests": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/tests",
      "sourceRoot": "projects/tests/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "functions/lib/ssr/tests/browser",
            "index": "projects/tests/src/index.html",
            "main": "projects/tests/src/main.ts",
            "polyfills": "projects/tests/src/polyfills.ts",
            "tsConfig": "projects/tests/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/tests/src/favicon.ico",
              "projects/tests/src/assets"
            ],
            "styles": [
              "projects/tests/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/tests/src/environments/environment.ts",
                  "with": "projects/tests/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tests:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tests:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tests:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/tests/src/test.ts",
            "polyfills": "projects/tests/src/polyfills.ts",
            "tsConfig": "projects/tests/tsconfig.spec.json",
            "karmaConfig": "projects/tests/karma.conf.js",
            "assets": [
              "projects/tests/src/favicon.ico",
              "projects/tests/src/assets"
            ],
            "styles": [
              "projects/tests/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/tests/tsconfig.app.json",
              "projects/tests/tsconfig.spec.json",
              "projects/tests/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/tests/e2e/protractor.conf.js",
            "devServerTarget": "tests:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tests:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "functions/lib/ssr/tests/server",
            "main": "projects/tests/server.ts",
            "tsConfig": "projects/tests/tsconfig.server.json",
            "externalDependencies": [
              "@firebase/firestore"
            ]
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "projects/tests/src/environments/environment.ts",
                  "with": "projects/tests/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "tests:build",
            "serverTarget": "tests:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "tests:build:production",
              "serverTarget": "tests:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "tests:build:production",
            "serverTarget": "tests:server:production",
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "ssr": true
          }
        }
      }
    },
    "ng-flamelink": {
      "projectType": "library",
      "root": "projects/ng-flamelink",
      "sourceRoot": "projects/ng-flamelink/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ng-flamelink/tsconfig.lib.json",
            "project": "projects/ng-flamelink/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-flamelink/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-flamelink/src/test.ts",
            "tsConfig": "projects/ng-flamelink/tsconfig.spec.json",
            "karmaConfig": "projects/ng-flamelink/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-flamelink/tsconfig.lib.json",
              "projects/ng-flamelink/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@fireplace/sdk": {
      "projectType": "library",
      "root": "projects/fireplace/sdk",
      "sourceRoot": "projects/fireplace/sdk/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/fireplace/sdk/tsconfig.lib.json",
            "project": "projects/fireplace/sdk/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/fireplace/sdk/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/fireplace/sdk/src/test.ts",
            "tsConfig": "projects/fireplace/sdk/tsconfig.spec.json",
            "karmaConfig": "projects/fireplace/sdk/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/fireplace/sdk/tsconfig.lib.json",
              "projects/fireplace/sdk/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "tests"
}